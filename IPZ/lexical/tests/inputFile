PROGRAM pr1;


LABEL 1, 2, 4, 5, 3, 12;

BEGIN


  1 : 
    IF var1 = 23 
      THEN 
        IF var2 = 45  
          THEN
            IF var3 = 13
              THEN 6 : GOTO 13;
        ELSE
          4 : GOTO 5;
    ENDIF;
    ENDIF;
  ENDIF;

END.

(**
PR1 SEGMENT

?1:
  cmp var1, 23
  jne THEN_1
  // ELSE
  JMP ?2

THEN_1:
  JMP ?4

?2:
  JPM ?3

?3: 
  NOP

?4: 
  JMP ?5

?5: 
  NOP
**)
